    "eslint-plugin-jsdoc": "18.4.3",
    "eslint-plugin-mocha": "6.2.2",
    "file-loader": "5.0.2",
    "husky": "3.1.0",
    "lerna": "3.19.0",
    "markdown-spellcheck": "1.3.1",
    "nodemon": "2.0.2",
    "ts-node": "8.5.4",
    "tslib": "1.10.0",
    "typescript": "3.7.4",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "7.7.7",
    "class-validator": "0.14.0",
    "es6-promise": "4.2.8"
