{
  "name": "@eneto/font-file-reader",
  "author": {
    "name": "Ernesto Jara Olveda",
    "email": "ernesto.jara06@gmail.com"
  },
  "version": "1.0.0",
  "private": true,
  "description": "adds Fira Code ligatures to other fonts.",
  "main": "./src/index.ts",
  "types": "./types/",
  "typings": "./src/typings/",
  "scripts": {
    "tsc:check": "tsc ./tsconfig.json",
    "tsc:trace": "tsc --traceResolution",
    "pretty": "prettier ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts --color --write",
    "fix": "eslint --config ./.eslintrc.js ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnetoJara/font-file-reader.git"
  },
  "keywords": [
    "ligatures"
  ],
  "license": "Apaache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/EnetoJara/font-file-reader/issues"
  },
  "workspaces": [
    "src/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": "12.14.0",
    "npm": "6.13.4"
  },
  "resolutions": {
